{
  "name": "@tancou/vue-redoc-wrapper",
  "version": "0.4.0",
  "description": "A Vue.js wrapper component for ReDoc",
  "main": "dist/vue-redoc-wrapper.umd.js",
  "unpkg": "dist/vue-redoc-wrapper.umd.min.js",
  "license": "MIT",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "NODE_OPTIONS=--max_old_space_size=4096 vue-cli-service build --target lib --name vue-redoc-wrapper src/main.js"
  },
  "dependencies": {
    "core-js": "^3.35.0",
    "vue": "^3.3.10"
  },
  "devDependencies": {
    "@vue/cli-service": "^5.0.8",
    "null": "^2.0.0",
    "redoc": "2.1.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
